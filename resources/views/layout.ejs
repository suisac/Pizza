<nav class="container flex justify-between items-center mx-auto">
    <div>
        <a href='/'>
            <img src="/img/logo.png" alt="logo" class="logo" style="width: 200px;">
        </a>
        
    </div>
    <div >
        <ul class="flex justify-between my-7">
            <li class="mx-6 nav-icon"><a href="/">Menu</a></li>
            <li class="mx-6 nav-icon"><a href="#">Offers</a></li>
            <%if(user){%>
                <li class="mx-6 nav-icon"><a href='/customers/orders'>My Orders</a></li>
            <li class="mx-6 nav-icon">

                <form id="logout" action="/logout" method="POST">
                    <a onclick="document.getElementById('logout').submit()" href="#">Log Out</a></li>
                </form>
            <%} else {%>
                <li class="mx-6 nav-icon"><a href="/register">Register</a></li>
            <li class="mx-6 nav-icon"><a href="/login">Login</a></li>
            <%}%>
            <li class="mx-6 px-3.5 py-1 cart-logo"><a href="/cart" class="flex items-center">
                <span class="cartCounter text-white pr-2"><%=session.cart? session.cart.totalQty : ''%></span>
                <img src="/img/cart.png" width="25px" ></a></li>
        </ul>
    </div>
</nav>
